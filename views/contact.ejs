  <%- include('header'); %>
    <div class="container-fluid">
      <div class="repeating-text-background z-n1">
        <div class="half-spacer"></div>
        <div class="row text-center">
          <h1 class="" style="font-size: 500%">contact</h1>
        </div>
        <div class="row">
          <form class="position-relative" id="contact-form">
            <div class="row my-3 justify-content-center mx-4">
              <div class="col-lg-3 col-md-4 col-sm-5">
                <div class="form-floating">
                  <input type="text" class="form-control" id="firstName" placeholder="first name"
                    aria-label="first name" style="justify-content: center;">
                  <label for="firstName">first name</label>
                </div>
              </div>
              <div class="col-lg-3 col-md-4 col-sm-5">
                <div class="form-floating">
                  <input type="text" class="form-control" id="lastName" placeholder="last name" aria-label="last name">
                  <label for="lastName">last name</label>
                </div>
              </div>
            </div>
            <div class="row mb-4 justify-content-center mx-4">
              <div class="col-lg-6 col-md-8 col-sm-10">
                <div class="form-floating">
                  <input type="email" class="form-control" placeholder="example@mail.com" id="inputEmail"
                    aria-describedby="emailHelp" required>
                  <label for="inputEmail">email address</label>
                  <div class="invalid-feedback">
                    please use the following email format, ex@example.com
                  </div>
                </div>
              </div>
            </div>
            <div class="row mb-3 justify-content-center mx-4">
              <div class="col-lg-6 col-md-8 col-sm-10">
                <textarea class="form-control solid-border" placeholder="your message here . . ." id="inputMessage"
                  rows="3"></textarea>
              </div>
            </div>
        </div>
        <div class="row mb-3 justify-content-center mx-4">
          <div class="col-lg-6 col-md-8 col-sm-10">
            <button id="contact-form-button" type="submit" class="btn btn-primary">Submit</button>
            <span class="m-2" id="contact-button-response"></span>
            <span class="m-2" id="result"></span>
          </div>
        </div>
        </form>
      </div>
    </div>
    <div class="half-spacer"></div>
    <%- include('footer'); %>

